{
    "name": "academe/omnipay-girocheckout",
    "type": "library",
    "description": "GiroSolution GiroCheckout driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "girosolution",
        "girocheckout"
    ],
    "homepage": "https://github.com/academe/omnipay-girocheckout",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason@academe.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Academe\\GiroCheckout\\" : "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
